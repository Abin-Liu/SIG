function getResist(skill,boss)
{
	if (!boss || !skill) return 100;
	var FireResit = boss.getStat(c_UnitStat_FireResist);
    	var LightResit = boss.getStat(c_UnitStat_LightningResist);
    	var ColdResit = boss.getStat(c_UnitStat_ColdResist);
	var PoisonResit = boss.getStat(c_UnitStat_PoisonResist);
	var PhyResit = boss.getStat(c_UnitStat_DamageResist);
	var MagicResit = boss.getStat(c_UnitStat_MagicResist);
	
	var sklType=getSkillType(skill);
	switch(sklType){
		case "Fire":
			return FireResit;
			break;
		case "Cold":
			return ColdResit;
			break;
		case "Light":
			if ( skill==35 && LightResit<100 ) LightResit = -50;
			return LightResit;
			break;
		case "Poison":
			return PoisonResit;
			break;
		case "Phy":
			return PhyResit;
			break;
		case "Magic":
			return (0.8 * PhyResit + 0.2 * MagicResit);
			break;
	}
	
	return 100;
}

function getSkillType(skill)
{
	if (!skill) return;
	
	switch(skill)
	{
		case 7:
		case 16:
		case 27:
			return "Fire";
			break;
		case 10:
		case 19:
		case 30:
		case 26:
		case 12:
		case 22: 
			return "Phy";
			break;
		case 11:
		case 21:
		case 31:
			return "Cold";
			break;
		case 6:
			return "Magic";
			break;
		case 15:
		case 25:
			return "Poison";
			break;
		case 14:
		case 24:
		case 20:
		case 34:
		case 35:
			return "Light";
	}
}

function sortskill(a,b)
{
	if (a[1] < b[1]) return 1;
	else return -1;
}
	

