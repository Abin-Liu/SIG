    var reval=cMap.PathMove(path,true,0,7,mlASTAR,true);
    if (reval != 0) {
    	if (!GotoRoom(51,52,20,7,true)) return false;
    	if (!TakeStairs(2,3)) return false;
    }
    
    var retries=0;
    while(me.area!=12 && retries<5){
    	delay(200);
    	retries++;
    }
    
    InitMap();
    
    var roomlist=new Array();
    var troom=getRoom();
